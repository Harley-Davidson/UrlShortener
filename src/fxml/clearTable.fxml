<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="110.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="controllers.ClearTableDialog">
    <children>
        <Label text="Are you sure you want to clear URL shortener history?">
            <VBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </VBox.margin>
        </Label>
        <HBox alignment="CENTER" prefHeight="49.0" prefWidth="405.0">
            <children>
                <Button fx:id="btnClearHistoryConfirm" alignment="CENTER" defaultButton="true" mnemonicParsing="false"
                        onAction="#clearTableConfirmed" prefWidth="130.0" text="Yes!">
                    <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="btnClearHistoryCancel" alignment="CENTER" cancelButton="true" contentDisplay="CENTER"
                        mnemonicParsing="false" onAction="#clearTableClose" prefWidth="130.0" text="No, not now">
                    <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
